.nowarp{
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
  }
  .tips{
    background: #f4f5f6;
    color: #000;
  }
  .tips_on{
      background: darkorange;
      color: white;
  }
  .text_runcate{
    overflow:hidden;
    text-overflow:ellipsis;
    display:-webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient:vertical;
  }
  .active{
      color: red;
  }

